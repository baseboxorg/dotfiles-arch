music_directory "~/Music/"
playlist_directory "~/.mpd/playlists"
db_file "~/.mpd/db"
log_file "~/.mpd/log"
pid_file "~/.mpd/pid"
state_file "~/.mpd/state"

#audio_output {
#    type "alsa"
#    name "HDA Intel HDMI"
#}

audio_output {
    type "pulse"
    name "pulse audio"
}

audio_output {
    type "fifo"
    name "FIFO"
    path "/tmp/mpd.fifo"
    format "44100:16:2"
}

bind_to_address "127.0.0.1"
port "6600"
